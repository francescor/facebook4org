<h1>Listing user_spoken_languages</h1>

<table>
  <tr>
    <th>User</th>
    <th>Language</th>
    <th>Rank</th>
  </tr>

<% @user_spoken_languages.each do |user_spoken_language| %>
  <tr>
    <td><%=h user_spoken_language.user_id %></td>
    <td><%=h user_spoken_language.language_id %></td>
    <td><%=h user_spoken_language.rank %></td>
    <td><%= link_to 'Show', user_spoken_language %></td>
    <td><%= link_to 'Edit', edit_user_spoken_language_path(user_spoken_language) %></td>
    <td><%= link_to 'Remove', user_spoken_language, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New user_spoken_language', new_user_spoken_language_path %>